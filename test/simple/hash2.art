trait Hash{
    fn hash() -> u32;
}

impl Hash for bool{
    fn hash()->u32{
        if(self) {1} else {0}
    }
}

impl Hash for u32{
    fn hash() -> u32 {self}
}

fn getHashable(i:i32) -> Hash{
  if(i < 0) {false as Hash} else{i as u32}
}

fn getHash(h:Hash)->u32{
    h.hash()
}

fn test(){
    getHash(getHashable(-2))
}