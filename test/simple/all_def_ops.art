fn test_i8(mut a:i8, mut b: i8) =
    (a + b,  a - b, a * b, a / b, a % b, a >> b, a << b,
    a & b, a | b, a ^ b,
    a < b, a <= b, a > b, a >= b, a == b, a != b,
    a += b,  a -= b, a *= b, a /= b, a %= b, a >>= b, a <<= b,
    ++a, --a, b++, b--, !a, &a, *&a, ?a, $a);

