static zero     = |_: fn () -> ()| {};
static one      = |f: fn () -> ()| f();
static two      = |f: fn () -> ()| { f(); f() };
static add      = |a: fn (fn () -> ()) -> (), b: fn (fn () -> ()) -> ()| |f: fn () -> ()| { a(f); b(f); };
static multiply = |a: fn (fn () -> ()) -> (), b: fn (fn () -> ()) -> ()| |f: fn () -> ()| { b(| | a(f)); };
