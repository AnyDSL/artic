fn test(a: i32, mut b: f32, c: bool) {
    let _ = a && 1;
    let _ = a || 1;
    let _ = b && 1.0;
    let _ = b || 1.0;
    let _ = b & 1.0;
    let _ = b | 1.0;
    let _ = b ^ 1.0;
    let _ = b >> 1.0;
    let _ = b << 1.0;
    let _ = c + true;
    let _ = c - true;
    let _ = c * true;
    let _ = c / true;
    let _ = c % true;
    let _ = c >> true;
    let _ = c << true;
    let _ = !b;
    let _ = ++b;
    let _ = --b;
}
